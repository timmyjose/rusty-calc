{
  "name": "rusty-calc",
  "Scheme": "RustyCalc",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "setup": "bash setup.sh",
    "eas-build-pre-install": "bash ./eas-hooks/eas-build-pre-install.sh && bash setup.sh",
    "eas-build-on-success": "bash ./eas-hooks/eas-build-on-success.sh",
    "start": "expo start",
    "android": "yarn setup && expo run:android",
    "android:prod": "yarn setup && expo run:android --variant release",
    "ios": "cd ios && npx pod-install && cd ../ && expo run:ios",
    "ios:prod": "cd ios && npx pod-install && cd ../ && expo run:ios --configuration Release",
    "web": "expo start --web",
    "lint": "eslint **/*.tsx"
  },
  "dependencies": {
    "@sentry/react-native": "5.17.0",
    "@types/react": "~18.2.14",
    "expo": "^50.0.4",
    "expo-build-properties": "~0.11.0",
    "expo-dev-client": "~3.3.7",
    "expo-splash-screen": "~0.26.4",
    "expo-status-bar": "~1.11.1",
    "react": "18.2.0",
    "react-native": "0.73.2",
    "react-native-rust-bridge": "*"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@config-plugins/detox": "^6.0.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.7.5",
    "@typescript-eslint/parser": "^6.7.5",
    "detox": "^20.16.0",
    "eslint": "^8.51.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-n": "^16.2.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.3"
  },
  "private": true
}
